generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Favorite {
  id         Int      @id @default(autoincrement())
  title      String
  type       String
  director   String?
  budget     String?
  location   String?
  duration   String?
  yearTime   String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
  posterUrl  String?
  notes      String?
}
